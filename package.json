{
  "name": "gatsby-starter-chakra-ui-type-script",
  "version": "1.0.1",
  "description": "A tribute to Nogbad the Bad",
  "author": "Stephen Harrison <stephen@harrison.org>",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "postinstall": "npm run theme",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "theme": "chakra-cli tokens src/theme/index.ts",
    "theme:watch": "chakra-cli tokens src/theme/index.ts --watch",
    "publish": "gatsby build && gh-pages -d public gh-pages"
  },
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^3.1.3",
    "@chakra-ui/icon": "^3.2.0",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.5.5",
    "@chakra-ui/system": "^2.6.2",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "framer-motion": "^11.0.0",
    "gatsby": "^5.11.0",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-react-helmet": "^6.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "gh-pages": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.0.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.3.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "typescript": "^5.0.2"
  }
}
